{
    "name": "magentopackage/vietech",
    "description": "",
    "authors": [
        {
            "name": "Pratik Rasam",
            "email": "pratikrasam5@gmail.com"
        }
    ],
    "require": {
             "php": ">=5.5.9",
            "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Magento\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",  
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Magento\\MagentoServiceProvider"
            ],
            "aliases": {
                "Magento": "Magento\\Facades\\MagentoFacade"
            }
        }
    }
}
